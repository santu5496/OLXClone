    // =================== CAR LISTINGS ===================
    function initCarListings() 
 
    {
          
        var table = $('#tblCarListings').DataTable();
        table.clear().draw();

        $.get('/VehicleManagement/GetCarListings', function (data) {

            data.forEach(function (item) {
                table.row.add([
                    item.listingId,
                    item.listingTitle,
                    item.brandId,
                    item.sellingPriceAsked,
                    FormatDateInReadableFormat(item.listingCreatedDate),
                    `<i class='fa fa-edit btn-edit' style='color:green' data-full='${JSON.stringify(item)}'></i>
                     <i class='fa fa-trash btn-delete' style='color:red' data-id='${item.listingId}'></i>`
                ]).draw();
            });
        });

        $('#tblCarListings').on('click', '.btn-edit', function () {
            var data = JSON.parse($(this).attr('data-full'));
            editListing(data);
        });

        $('#tblCarListings').on('click', '.btn-delete', function () {
            var id = $(this).data('id');
            DeleteWarning(deleteListing, id);
        });
    }

    function clearFeatureModal() {
     
        $('.ClearListingModalData').val('');
        $('#listingModal').modal('show');
    }

    function saveListingold() {
        if (CheckEmptyFields('Listing')) {
            let data = {
                listingId: $('#listingId').val(),
                listingTitle: $('#listingTitle').val(),
                brandId: $('#brandId').val(),
                sellingPriceAsked: $('#sellingPrice').val()
            };
            $.post('/VehicleManagement/AddOrUpdateCarListing', data, function (res) {
                msgPopup('success', 'Saved successfully');
                $('#listingModal').modal('hide');
                initCarListings();
            });
        }
    }

    function editListing(data) {
        $('#listingId').val(data.listingId);
        $('#listingTitle').val(data.listingTitle);
        $('#brandId').val(data.brandId);
        $('#sellingPrice').val(data.sellingPriceAsked);
        $('#listingModal').modal('show');
    }

    function deleteListing(id) {
        $.post('/VehicleManagement/DeleteCarListing', { id: id }, function (res) {
            msgPopup('success', 'Deleted');
            initCarListings();
        });
    }

</script>
